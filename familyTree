    <script>
        function navFunction() {
            var x = document.getElementById("dropmenu");
            // Check if Tailwind's 'hidden' class is being used or if direct style manipulation is better
            if (x.classList.contains("hidden")) { // Assuming mobile nav starts hidden by Tailwind
                x.classList.remove("hidden");
                x.classList.add("block", "absolute", "w-full", "left-0", "top-full", "shadow-lg", "md:hidden"); // Example mobile dropdown style
                x.style.backgroundColor = '#A3B18A'; // Match nav color
            } else {
                x.classList.add("hidden");
                x.classList.remove("block", "absolute", "w-full", "left-0", "top-full", "shadow-lg", "md:hidden");
            }
        }
    </script>
